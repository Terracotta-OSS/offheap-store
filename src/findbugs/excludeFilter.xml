<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Locking on CHM all mutations of "lookup" map to perform a large atomic operation -->
	<Match>
		<Class name="com.terracottatech.offheapstore.storage.portability.SerializablePortability" />
		<Method name="getOrAddMapping" params="java.io.ObjectStreamClass" returns="int" />
		<Bug pattern="JLM_JSR166_UTILCONCURRENT_MONITORENTER" />
	</Match>
    <Match>
        <Class name="com.terracottatech.offheapstore.disk.storage.portability.PersistentSerializablePortability" />
        <Method name="bootstrap" params="java.io.ObjectInput" returns="void" />
        <Bug pattern="JLM_JSR166_UTILCONCURRENT_MONITORENTER" />
    </Match>
    <Match>
        <Class name="com.terracottatech.offheapstore.disk.storage.portability.PersistentSerializablePortability" />
        <Method name="persist" params="java.io.ObjectOutput" returns="void" />
        <Bug pattern="JLM_JSR166_UTILCONCURRENT_MONITORENTER" />
    </Match>
</FindBugsFilter>

